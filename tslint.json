{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "import-name": [false],
    "function-name": [false],
    "no-submodule-imports": [false],
    "jsx-boolean-value": [false],
    "object-literal-sort-keys": [false],
    "variable-name": [false]
  }
}
